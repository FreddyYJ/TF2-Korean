msgstr "방황"

msgid "War material"
msgstr "전쟁 물자"

msgid "Waste water"
msgstr "폐수"

msgid "Water"
msgstr "물"

msgid "Water Dirty"
msgstr "물 오염도"

msgid "Waypoint"
msgstr "웨이포인트"

msgid "Welcome"
msgstr "환영합니다"

