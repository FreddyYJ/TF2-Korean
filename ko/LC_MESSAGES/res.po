msgid "MISSION_SWISSMADE_PARAMS_FOODPROCESSING_N_NAME"
msgstr "로만스호른 음식 가공 공장"

msgid "MISSION_SWISSMADE_PARAMS_FOODPROCESSING_N_STATION_NAME"
